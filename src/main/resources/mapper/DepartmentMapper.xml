<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gdpu.springbootcrud.mapper.DepartmentMapper">
    <select id="mySelectById" parameterType="Long" resultType="Department">
        select did, dept_name as deptName, description from department where did = #{did}
    </select>
</mapper>
